<div>
  <h4>sign in</h4>
  <%= form_with url: '/sign_in_complete', method: :post do |form| %>
    <div>
      <%= form.label :email, style: "display: block" %>
      <%= form.text_field :email %>
    </div>

    <div>
      <%= form.label :password, style: "display: block" %>
      <%= form.text_field :password %>
    </div>

    <div>
      <%= form.submit %>
    </div>
  <% end %>

<%= link_to "sign up", sign_up_path %>
</div>
